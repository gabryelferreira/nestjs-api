{
    "type": "sqlite",
    "database": "dist/nestjs-prod.db",
    "entities": [
        "dist/**/*.entity{.ts,.js}"
    ],
    "migrationsTableName": "custom_migration",
    "migrations": [
        "dist/db/migrations/*{.ts,.js}"
    ],
    "cli": {
        "migrationsDir": "dist/db/migrations"
    }
}